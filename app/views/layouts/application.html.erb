<!DOCTYPE html>
<html>
  <head>
    <title>TestCenter</title>
    <%= csrf_meta_tags %>
    <%= render 'layouts/js' %>
  </head>

  <body>
  <% if current_user %>
    <%= render 'layouts/header' %>
    <%= render 'layouts/page_navbar' %>
